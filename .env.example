# AI Provider設定
# 使用するプロバイダーを選択: claude | openai | gemini
AI_PROVIDER=claude

# API Keys（使用するプロバイダーのキーのみ必須）
ANTHROPIC_API_KEY=your_api_key_here   # Claude API用
OPENAI_API_KEY=your_api_key_here      # OpenAI GPT-4V用
GOOGLE_API_KEY=your_api_key_here      # Google Gemini用

# Flask設定
FLASK_ENV=development
FLASK_DEBUG=1
SECRET_KEY=your_secret_key_here

# アップロード設定
MAX_UPLOAD_SIZE=10485760
